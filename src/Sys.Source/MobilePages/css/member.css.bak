    background-color: #fff;
    overflow: hidden;
}
.bdb-1px::after {
    bottom: 0;
}
.bdb-1px::after {
    border-top: 1px solid #e0e0e0;
    content: " ";
    display: block;
    left: 0;
    position: absolute;
    width: 100%;
}
.s-list .s-item {
    padding: 12px 10px 12px;
}
.bdb-1px {
    position: relative;
}
.s-item {
